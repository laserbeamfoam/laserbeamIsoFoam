FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     Flint_multiphaseEulerFoamD;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         100e-6;

deltaT          1.0e-8;

writeControl    adjustableRunTime;

writeInterval   0.000001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.5;//0.1;
maxAlphaCo      0.5;

maxDeltaT       2e-8;


